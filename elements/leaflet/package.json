{
  "name": "@custom-element/leaflet",
  "version": "0.2.10",
  "description": "leaflet map",
  "main": "dist/main.min.js",
  "scripts": {
    "clean": "rm -rf dist && mkdir dist",
    "package": "webpack main.js --output-filename=dist/main.js",
    "min-js": "uglifyjs dist/main.js -m reserved=[LeafletMap,LeafletMarker] -c > dist/main.min.js",
    "build": "npm-run-all clean package min-js",
    "doc": "jsdoc2md *.js > DOC-GENERATED.md",
    "build-and-publish": "npm run build && npm version patch && npm publish --access=public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/allenhwkim/custom-element/elements"
  },
  "keywords": [
    "custom element",
    "web component",
    "html",
    "leaflet"
  ],
  "files": [
    "dist"
  ],
  "author": "allenhwkim@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "jsdoc-to-markdown": "^3.0.2",
    "npm-run-all": "^4.1.1",
    "uglify-es": "^3.1.7",
    "uglifycss": "0.0.27",
    "webpack": "^3.8.1"
  }
}
